<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <ahrc-template j:dependencies="ahrc-banner ahrc-disclaimer ahrc-dynamic-quick-links ahrc-footer ahrc-header ahrc-quick-links ahrc-text-resizer bootstrap bootstrap-components default search sitemap test-jcr-1"
                     j:installedModules="ahrc-template"
                     j:modulePriority="0"
                     j:moduleType="templatesSet"
                     j:title="AHRC Template"
                     jcr:mixinTypes="jmix:hasExternalProviderExtension"
                     jcr:primaryType="jnt:module">

            <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder">
                                <logoNew.gif j:height="44"
                                  j:width="426"
                                  jcr:mixinTypes="jmix:image"
                                  jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                </logoNew.gif>
                                <logoFooter.gif j:height="47"
                                     j:width="103"
                                     jcr:mixinTypes="jmix:image"
                                     jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                </logoFooter.gif>
                                <mainNav_bottom.jpg j:height="12"
                                         j:width="200"
                                         j:xresolution="72 dots"
                                         j:yresolution="72 dots"
                                         jcr:mixinTypes="jmix:exif jmix:image"
                                         jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                </mainNav_bottom.jpg>
                                <mainBanner.jpg j:height="156"
                                     j:width="710"
                                     j:xresolution="0 dots"
                                     j:yresolution="0 dots"
                                     jcr:mixinTypes="jmix:exif jmix:image"
                                     jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                </mainBanner.jpg>
                                <quicklinks_bottom.gif j:height="15"
                                            j:width="175"
                                            jcr:mixinTypes="jmix:image"
                                            jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                </quicklinks_bottom.gif>
                                <ahrccLogo.gif j:height="109"
                                    j:width="162"
                                    jcr:mixinTypes="jmix:image"
                                    jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/gif" jcr:primaryType="jnt:resource"/>
                                </ahrccLogo.gif>
                                <hr_educationFund.jpg j:height="90"
                                           j:width="195"
                                           j:xresolution="72 dots"
                                           j:yresolution="72 dots"
                                           jcr:mixinTypes="jmix:exif jmix:image"
                                           jcr:primaryType="jnt:file"
                                           jcr:title="ahrc-logo-oct-2009-large">
                                    <jcr:content jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/jpeg" jcr:primaryType="jnt:resource"/>
                                </hr_educationFund.jpg>
                            </default>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/ahrc-template/templates/files/themes/default"
                  j:view="ahrc-template"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                    <home jcr:primaryType="jnt:pageTemplate"/>
                    <simple jcr:primaryType="jnt:pageTemplate"/>
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <ahrc-bootstrap-container fluid="false" jcr:primaryType="jnt:bootstrapContainer">
                                <ahrc-bootstrap-container jcr:primaryType="jnt:contentList">
                                    <ahrc-header image="/modules/ahrc-template/templates/files/themes/default/logoNew.gif"
                                     jcr:primaryType="ahrc:ahrcHeader">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="AHRC Header"/>
                                    </ahrc-header>
                                    <searchandtextresizer jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                              jcr:primaryType="jnt:bootstrapFullComponent"
                                              layout="10_2"
                                              rowType="predefined">
                                        <searchandtextresizer jcr:primaryType="jnt:contentList">
                                            <ahrc-search jcr:primaryType="jnt:simpleSearchForm"
                                           result="/sites/ahrc/ahrc-search-results">
                                                <j:translation_en jcr:language="en"
                                                   jcr:mixinTypes="mix:title"
                                                   jcr:primaryType="jnt:translation"
                                                   jcr:title="AHRC Search"/>
                                            </ahrc-search>
                                        </searchandtextresizer>
                                        <searchandtextresizer-side1 jcr:primaryType="jnt:contentList">
                                            <ahrc-text-resizer jcr:primaryType="ahrc:ahrcTextResizer"/>
                                        </searchandtextresizer-side1>
                                    </searchandtextresizer>
                            </ahrc-bootstrap-container>
                            </ahrc-bootstrap-container>
                            <ahrc-bootstrap-breadcrumb displayCurrentPage="true"
                                             displayHome="true"
                                             displayLinkOnCurrentPage="false"
                                             displayOnFirstLevel="false"
                                             divider="&gt;"
                                             jcr:primaryType="jnt:bootstrapBreadcrumb"/>
                            <ahrc-banner bannerLogo="/modules/ahrc-template/templates/files/themes/default/ahrccLogo.gif"
                               bannerMainImage="/modules/ahrc-template/templates/files/themes/default/mainBanner.jpg"
                               jcr:primaryType="ahrc:ahrcBanner">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="AHRC Banner"/>
                            </ahrc-banner>
                            <bootstrap-layer-container-ahrc jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                                  jcr:primaryType="jnt:bootstrapFullComponent"
                                                  layout="3_6_3"
                                                  rowType="predefined">
                                <bootstrap-layer-container-ahrc-side1 jcr:primaryType="jnt:contentList">
                                    <bootstrap-navigation-menu j:baselineNode="home"
                                                   j:layoutID="navColumn"
                                                   j:maxDepth="4"
                                                   j:menuItemView="menuElement"
                                                   j:startLevel="0"
                                                   j:view="ahrcNavMenu"
                                                   jcr:mixinTypes="jmix:renderable"
                                                   jcr:primaryType="jnt:bootstrapNavMenu">
                                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation"/>
                                    </bootstrap-navigation-menu>
                                </bootstrap-layer-container-ahrc-side1>
                                <bootstrap-layer-container-ahrc jcr:primaryType="jnt:contentList">
                                    <MainContent j:allowedTypes="jmix:droppableContent"
                                     j:areaView="default"
                                     jcr:primaryType="jnt:area"/>
                                    <ahrc-disclaimer jcr:primaryType="ahrc:ahrcDisclaimer"/>
                                </bootstrap-layer-container-ahrc>
                                <bootstrap-layer-container-ahrc-side2 jcr:primaryType="jnt:contentList">
                                    <quick-links-sec j:channelIncludeOrExclude="include"
                                         j:channelSelection="generic_android"
                                         j:invertCondition="false"
                                         j:requireLoggedUser="false"
                                         j:requirePrivilegedUser="false"
                                         jcr:mixinTypes="jmix:channelSelection jmix:requiredPermissions"
                                         jcr:primaryType="ahrc:ahrcquicklinksdyn">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Quick Links Sec"/>
                                        <contact-us j:node="/sites/ahrc/ahrc-home/about-the-commission/contact-us"
                                       j:target="_self"
                                 jcr:primaryType="jnt:nodeLink">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Contact Us"/>
                                        </contact-us>
                                        <faqs j:node="/sites/ahrc/ahrc-home/about-the-commission/faqs"
                                 jcr:primaryType="jnt:nodeLink">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="FAQs"/>
                                        </faqs>
                                        <mandate j:node="/sites/ahrc/ahrc-home/about-the-commission/our-mandate"
                                    jcr:primaryType="jnt:nodeLink">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Mandate"/>
                                        </mandate>
                                        <about-us j:node="/sites/ahrc/ahrc-home/about-the-commission"
                                     jcr:primaryType="jnt:nodeLink">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="About Us"/>
                                        </about-us>
                                        <code-of-conduct j:node="/sites/ahrc/ahrc-home/about-the-commission/our-mandate/code-of-conduct"
                                            jcr:primaryType="jnt:nodeLink">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Code of Conduct"/>
                                        </code-of-conduct>
                                        <decisions j:node="/sites/ahrc/ahrc-home/decisions" jcr:primaryType="jnt:nodeLink">
                                            <j:translation_en jcr:language="en"
                                                jcr:mixinTypes="mix:title"
                                                jcr:primaryType="jnt:translation"
                                                jcr:title="Decisions"/>
                                        </decisions>
                                    </quick-links-sec>
                                    <QuickLinksListArea j:allowedTypes="ahrc:ahrcquicklinksdyn"
                                             jcr:primaryType="jnt:absoluteArea"/>
                                </bootstrap-layer-container-ahrc-side2>
                            </bootstrap-layer-container-ahrc>
                            <ahrc-footer footerImage="/modules/ahrc-template/templates/files/themes/default/logoFooter.gif"
                               jcr:primaryType="ahrc:ahrcFooter"/>
                        </pagecontent>
                </base>
                    <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="ahrc-template"
                              jcr:primaryType="jnt:contentTemplate">
                    <pagecontent jcr:primaryType="jnt:contentList">
                            <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                    </pagecontent>
                </content-template>
            </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
        </ahrc-template>
    </modules>
</content>
